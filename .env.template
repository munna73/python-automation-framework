# Environment Variables Template
# Copy this to .env and fill in your actual values

# Oracle Database Passwords
DEV_ORACLE_PWD=your_dev_oracle_password
QA_ORACLE_PWD=your_qa_oracle_password
PROD_ORACLE_PWD=your_prod_oracle_password

# PostgreSQL Database Passwords
DEV_POSTGRES_PWD=your_dev_postgres_password
QA_POSTGRES_PWD=your_qa_postgres_password
PROD_POSTGRES_PWD=your_prod_postgres_password

# MongoDB Database Passwords
DEV_MONGODB_PWD=your_dev_mongodb_password
QA_MONGODB_PWD=your_qa_mongodb_password
PROD_MONGODB_PWD=your_prod_mongodb_password

# API Authentication
API_TOKEN=your_api_bearer_token

# MQ Authentication
MQ_PWD=your_mq_password

# Logging Level
LOG_LEVEL=INFO